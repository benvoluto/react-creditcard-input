{
  "name": "react-creditcard-input",
  "version": "0.0.1",
  "description": "Credit card input as react component ",
  "main": "src/app.jsx",
  "repository": {
    "type": "git",
    "url": "git@github.com:eniz/react-creditcard-input.git"
  },
  "keywords": [
    "react-component",
    "react",
    "component",
    "react-creditcard-input",
    "credit card",
    "flux"
  ],
  "authors": [
    "Eniz Gulek<enizgulek@gmail.com> (http://enizgulek.com)"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "start": "npm run build && serve .",
    "build": "npm run build-js",
    "watch": "npm run watch-js & npm run watch-css & serve .",
    "test": "npm run build",
    "deploy": "gh-pages-deploy",
    "build-js": "browserify --extension=.jsx --extension=.js src/app.jsx | uglifyjs > build/build.js",
    "watch-js": "watchify --extension=.jsx --extension=.js src/app.jsx -o build/build.js --debug --verbose",
    "lint-eslint": "eslint .",
    "lint-jscs": "jscs .",
    "lint": "npm run lint-eslint && npm run lint-jscs"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eniz/react-creditcard-input/issues"
  },
  "homepage": "https://github.com/eniz/react-creditcard-input",
  "dependencies": {
    "bootstrap": "^3.3.0",
    "flux": "^2.0.1",
    "react": "^0.13.1"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browser-sync": "^2.1.6",
    "browserify": "^8.0.3",
    "eslint": "^0.14.1",
    "gh-pages-deploy": "^0.3.0",
    "nodemon": "^1.3.7",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "watchify": "^2.1.1"
  }
}
